namespace ama {
	#pragma add("jc_files", "./charset.jc");
	inline uint32_t! isInCharSet(uint32_t[...]! cset, uint32_t! ch) {
		ch &= 0xffu;
		return cset[ch >> 5] >> (ch & 31u) & 1u;
	}
	char*! SkipChars(char*! feed, uint32_t[...]! cset);
	uint32_t[8]! CreateCharSet(char*! s);
};
